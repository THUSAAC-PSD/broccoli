{
    "name": "@broccoli/sdk",
    "version": "0.1.0",
    "private": false,
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        },
        "./react": {
            "types": "./dist/react.d.ts",
            "import": "./dist/react.js"
        },
        "./i18n": {
            "types": "./dist/i18n.d.ts",
            "import": "./dist/i18n.js"
        },
        "./api": {
            "types": "./dist/api.d.ts",
            "import": "./dist/api.js"
        },
        "./plugin": {
            "types": "./dist/plugin.d.ts",
            "import": "./dist/plugin.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup src/index.ts src/react.tsx src/i18n.tsx src/api.ts src/plugin.ts --format esm --dts",
        "dev": "tsup src/index.ts src/react.tsx src/i18n.tsx src/api.ts src/plugin.ts --format esm --dts --watch"
    },
    "devDependencies": {
        "@types/react": "^19.1.16",
        "tsup": "^8.5.0",
        "typescript": "~5.9.3"
    },
    "peerDependencies": {
        "react": "^18.0.0 || ^19.0.0"
    },
    "dependencies": {
        "openapi-fetch": "^0.16.0",
        "react": "^19.2.0"
    }
}
